      return false;
    }

	StringBuffer scriptOutput=new StringBuffer();
	String NODE_ID = get(Fields.In, "NODE_ID").getString(r);
	String IP = get(Fields.In, "IP").getString(r);
	int PORT = Integer.parseInt(get(Fields.In, "PORT").getString(r));
	String USERNAME = get(Fields.In, "USERNAME").getString(r);
	String PASSWORD = get(Fields.In, "PASSWORD").getString(r);
	String MIG_TOOL_PATH = get(Fields.In, "MIG_TOOL_PATH").getString(r);

	JSch jsch=null;
	Session session=null;
	java.util.Properties config=null;
	ChannelSftp sftpChannel=null;
	Channel channel=null;
	InputStream in=null,error=null;

	try
	{
		jsch=new JSch();
		session=jsch.getSession(USERNAME, IP, PORT);
			session.setPassword(PASSWORD);

		config = new java.util.Properties(); 
		config.put("StrictHostKeyChecking", "no");
		session.setConfig("PreferredAuthentications", "password");

		scriptOutput.append("Node="+NODE_ID+", Host IP="+IP+", Port="+PORT+",Username="+USERNAME+", Remote Migtool Path="+MIG_TOOL_PATH+"report/subscriber/"+", Local Path="+kpi_path+"\n");	

		session.setConfig(config);
		session.connect();

		channel=session.openChannel("sftp");
	    session.setServerAliveInterval(92000);
		channel.connect();
		
		sftpChannel = (ChannelSftp) channel;

		sftpChannel.cd(MIG_TOOL_PATH+"report/subscriber/");
		
		Vector lsEntries = sftpChannel.ls("*.txt");

		logBasic("Downloading xls Reports from Node="+NODE_ID+", Host IP="+IP+", Port="+PORT+",Username="+USERNAME+", Remote Migtool Path="+MIG_TOOL_PATH+"report/subscriber/"+", Local Path="+kpi_path);	
		for (int i=0;i&lt;lsEntries.size();i++)
		{
			String temp=((LsEntry)lsEntries.get(i)).getFilename();
			
			 File theDir = new File(kpi_path+ "rpt_" +NODE_ID+"/");
			 // if the directory does not exist, create it
			if (!theDir.exists()) 
			{
				try
				{
					theDir.mkdir();
					sftpChannel.get(temp, kpi_path+"rpt_" +NODE_ID+"/");
				} 
				catch(SecurityException se)
				{
					//handle it
				}  
			}
			else
			{
				sftpChannel.get(temp, kpi_path+"rpt_" +NODE_ID+"/");
			}
			
			logBasic("File Downloaded : "+temp+" TO LOCATION "+kpi_path+NODE_ID);
		}

		/*Vector lsEntries1 = sftpChannel.ls("*.csv");

		logBasic("Downloading CSV Reports from Node="+NODE_ID+", Host IP="+IP+", Port="+PORT+",Username="+USERNAME+", Remote Migtool Path="+MIG_TOOL_PATH+", Local Path="+local_path);	
		for (int i=0;i&lt;lsEntries1.size();i++)
		{
			String temp=((LsEntry)lsEntries1.get(i)).getFilename();
			
			File theDir = new File(local_path);
			// if the directory does not exist, create it
			if (!theDir.exists()) 
			{
				try
				{
					theDir.mkdir();
					sftpChannel.get(temp, local_path);
				} 
				catch(SecurityException se){
					//handle it
				}  
			}
			else
			{
				sftpChannel.get(temp, local_path);
			}
			
			logBasic("		File Downloaded : "+temp+" TO LOCATION "+local_path);
		}*/
		
		sftpChannel.exit();
		session.disconnect();
		
	}
	catch(Exception exception)
	{
		scriptOutput.append(exception+"\n");
		logError(exception.getMessage(), exception);
	}
	finally
	{
		scriptOutput.append("-----------------------------------------------------------------");
		r = createOutputRow(r, data.outputRowMeta.size());
        get(Fields.Out, "STATUS").setValue(r, scriptOutput.toString());
		putRow(data.outputRowMeta, r);
		
		jsch=null;
		config=null;
		session=null;
		sftpChannel=null;
		in=null;error=null;
		scriptOutput=null;
	}
	return true;
}


public boolean init(StepMetaInterface stepMetaInterface, StepDataInterface stepDataInterface)
{
	super.init(stepMetaInterface, stepDataInterface);
	kpi_path=getVariable("KETTLE_HOME","")+"/report/subscriber/";
	output_dir=getVariable("OUTPUT_DIR","")+"/";
	report_dir=getVariable("REPORTS_DIR","")+"/";
	return true;
}

public void dispose(StepMetaInterface smi, StepDataInterface sdi)
{
    parent.disposeImpl(smi, sdi);
