	public void readSubuscriber(String filePath) {
	    //List<String> lines = new ArrayList<>();
	    int counter = 0;
		File file = new File(filePath);

	    try (GZIPInputStream gzip = new GZIPInputStream(new FileInputStream(file));
	            BufferedReader br = new BufferedReader(new InputStreamReader(gzip));) {
	        String line = null;
	        while ((line = br.readLine()) != null) {
	        	businessLogicImplementation(line);	  
	        	
	        }
	        generateCsv();
	    } catch (FileNotFoundException e) {
	        e.printStackTrace(System.err);
	    } catch (IOException e) {
	        e.printStackTrace(System.err);
	    } catch (JiBXException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}	    
	}
	
	public void generateCsv() {
		//synchronized (ThreadLock)
		{
			// TODO Auto-generated method stub
			/*System.out.println("Going to Write Output for subs: " + this.subscriberListBuffer.size());
			System.out.println("Going to Write Output for offerAtr: " + this.offerAttributeListBuffer.size());
			System.out.println("Going to Write Output for offer: " + this.offerListBuffer.size());
			System.out.println("Going to Write Output for Account: " + this.accountListBuffer.size());
			System.out.println("Going to Write Output for logs: " + onlyLog.size());*/
			try {
				File accountFile = new File(this.pathOfOutputFolder + "Account_" + uniqueNumber + ".csv");
				File subscriberFile = new File(this.pathOfOutputFolder + "Subscriber_" + uniqueNumber + ".csv");
				File offerFile = new File(this.pathOfOutputFolder + "Offer_" + uniqueNumber + ".csv");
				File offerAttrFile = new File(this.pathOfOutputFolder + "OfferAttribute_" + uniqueNumber + ".csv");
				File pamFile = new File(this.pathOfOutputFolder + "PamAccount_" + uniqueNumber + ".csv");
				File accumulatorFile = new File(this.pathOfOutputFolder + "Accumulator_" + uniqueNumber + ".csv");
				File cisOnceOffFile = new File(this.pathOfOutputFolder + "CIS_OnceOff_Bundles_" + uniqueNumber + ".csv");
				File cisRenewalFile = new File(this.pathOfOutputFolder + "CIS_Renewal_Bundles_" + uniqueNumber + ".csv");
				File cisParkingFile = new File(this.pathOfOutputFolder + "CIS_Parking_Products_" + uniqueNumber + ".csv");
				File cisNwpcFile = new File(this.pathOfOutputFolder + "CIS_Notification_" + uniqueNumber + ".csv");
				File servicenumberappFile = new File(this.pathOfOutputFolder + "Service_Number_Application_" + uniqueNumber + ".csv");
				File subscriberFafFile = new File(this.pathOfOutputFolder + "SubscriberFaf_" + uniqueNumber + ".csv");
				File RoundOffFile = new File(this.pathOfOutputFolder + "RoundOff_Records_" + uniqueNumber + ".csv");
				// File bigSubsFile = new File(this.pathOfOutputFolder +
				// "BigSubscriber.txt");
	
				File ucFile = new File(this.pathOfOutputFolder + "UsageCounter_" + uniqueNumber + ".csv");
				File daFile = new File(this.pathOfOutputFolder + "DedicatedAccount_" + uniqueNumber + ".csv");
				
				File rejectedFile = new File(this.pathOfOutputFolder + "Rejected_" + uniqueNumber +".log");
				File onlyLogFile = new File(this.pathOfOutputFolder + "Exception_" + uniqueNumber + ".log");
				File trackLogFile = new File(this.pathOfOutputFolder + "Track_" + uniqueNumber + ".log");
				
				if (!accountFile.exists()) {
					accountFile.createNewFile();
				}
				if (!subscriberFile.exists()) {
					subscriberFile.createNewFile();
				}
				if (!offerFile.exists()) {
					offerFile.createNewFile();
				}
				if (!offerAttrFile.exists()) {
					offerAttrFile.createNewFile();
				}
				if (!pamFile.exists()) {
					pamFile.createNewFile();
				}
				if (!ucFile.exists()) {
					ucFile.createNewFile();
				}
				if (!daFile.exists()) {
					daFile.createNewFile();
				}
				if (!rejectedFile.exists()) {
					rejectedFile.createNewFile();
				}
				if (!onlyLogFile.exists()) {
					onlyLogFile.createNewFile();
				}
				if (!accumulatorFile.exists()) {
					accumulatorFile.createNewFile();
				}
				if (!cisOnceOffFile.exists()) {
					cisOnceOffFile.createNewFile();
				}
				if (!cisRenewalFile.exists()) {
					cisRenewalFile.createNewFile();
				}
				if (!cisParkingFile.exists()) {
					cisParkingFile.createNewFile();
				}
				if (!cisNwpcFile.exists()) {
					cisNwpcFile.createNewFile();
				}
				if (!servicenumberappFile.exists()) {
					servicenumberappFile.createNewFile();
				}
				if (!subscriberFafFile.exists()) {
					subscriberFafFile.createNewFile();
				}
				if (!rejectedFile.exists()) {
					rejectedFile.createNewFile();
				}		
				if (!onlyLogFile.exists()) {
					onlyLogFile.createNewFile();
				}
				if (!RoundOffFile.exists()) {
					RoundOffFile.createNewFile();
				}
				if (!trackLogFile.exists()) {
					trackLogFile.createNewFile();
				}
				/*
				 * if(!bigSubsFile.exists()){ bigSubsFile.createNewFile(); }
				 */
				FileUtils.writeLines(accountFile, this.accountListBuffer, true);
				FileUtils.writeLines(subscriberFile, this.subscriberListBuffer, true);
				FileUtils.writeLines(offerFile, this.offerListBuffer, true);
				FileUtils.writeLines(offerAttrFile, this.offerAttributeListBuffer, true);
				FileUtils.writeLines(pamFile, this.pamListBuffer, true);
				FileUtils.writeLines(ucFile, this.ucBuffer, true);
				FileUtils.writeLines(daFile, this.daBuffer, true);
				FileUtils.writeLines(accumulatorFile, this.accumulatorBuffer, true);
				FileUtils.writeLines(cisOnceOffFile, this.cisOnceOff, true);
				FileUtils.writeLines(cisRenewalFile, this.cisRenewal, true);
				FileUtils.writeLines(cisParkingFile, this.cisParking, true);
				FileUtils.writeLines(cisNwpcFile, this.cisNwpc, true);
				FileUtils.writeLines(subscriberFafFile, this.subscriberFaFBuffer, true);
				FileUtils.writeLines(servicenumberappFile, this.servicenumberBuffer, true);
				FileUtils.writeLines(RoundOffFile, this.roundOffLog, true);
				
				//Remove duplicates from files			
				synchronized (ThreadLock)
				{
					FileUtils.writeLines(rejectedFile, rejectAndLog, true);
					FileUtils.writeLines(onlyLogFile, onlyLog, true);
					FileUtils.writeLines(trackLogFile, trackLog, true);					
				}
				//onlyLog.forEach(action->{System.out.print(action);});
		
							
				rejectAndLog.clear();
				onlyLog.clear();
				// FileUtils.writeLines(bigSubsFile, this.smallFiles, true);
			} catch (IOException e) {
				// TODO Auto-generated catch block
				e.printStackTrace();
				LOG.error("IO Exception ", e);
			}
		}
	}
	
	public synchronized void generateLogs(){
		try 
		{
			File rejectedFile = new File(this.pathOfOutputFolder + "Rejected_" + uniqueNumber +".log");
			File onlyLogFile = new File(this.pathOfOutputFolder + "Exception_" + uniqueNumber + ".log");
			if (!rejectedFile.exists()) {
				rejectedFile.createNewFile();
			}		
			if (!onlyLogFile.exists()) {
				onlyLogFile.createNewFile();
			}
			FileUtils.writeLines(rejectedFile, rejectAndLog, true);
			FileUtils.writeLines(onlyLogFile, onlyLog, true);
		
			clearLogs();
		} catch (IOException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
			LOG.error("IO Exception ", e);
		}	
				
	}
	
	public synchronized void clearLogs() throws IOException{
		rejectAndLog.clear();
		onlyLog.clear();
	}

	public synchronized void  clearBuffer() {
		this.accountListBuffer.clear();
		this.subscriberListBuffer.clear();
		this.offerListBuffer.clear();
		this.offerAttributeListBuffer.clear();
		this.pamListBuffer.clear();
		this.accumulatorBuffer.clear();
		this.ucBuffer.clear();		
		this.daBuffer.clear();
		this.cisOnceOff.clear();
		this.cisRenewal.clear();
		this.cisParking.clear();
		this.cisNwpc.clear();
		this.subscriberFaFBuffer.clear();
		this.servicenumberBuffer.clear();
		
		rejectAndLog.clear();
		onlyLog.clear();
		trackLog.clear();
		
	}

