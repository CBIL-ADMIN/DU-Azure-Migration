	public synchronized void businessLogicImplementation(String xml) throws JiBXException, IOException {
		//synchronized (ThreadLock)
		{		
			try {
				
				//System.out.println(xml);
				//System.out.println("Start of transformation Thread: " + Thread.currentThread().getName() + " ThreadID: " + Thread.currentThread().getId());
				
				subs = (SubscriberXml) UnmarshallingContextSubuscriber.unmarshalDocument(new ByteArrayInputStream(xml.getBytes()), null);
				
				String MSISDN = subs.getSubscriberInfoMSISDN();
				String CCS_ACCT_TYPE_ID = subs.getSubscriberInfoCCSACCTTYPEID();
				String SERVICE_STATE = subs.getSubscriberInfoSERVICESTATE();
				//getting intial_activation_date form USMS input
				if(subs.getUsmsdumpInfoList().size() == 1 && subs.getUsmsdumpInfoList().get(0).getINITIALACTIVATIONDATE() != null)
				{
					this.INITIAL_ACTIVATION_DATE = subs.getUsmsdumpInfoList().get(0).getINITIALACTIVATIONDATE();
				}
				
				String INITIAL_ACTIVATION_DATE_FLAG;
				
				if(this.INITIAL_ACTIVATION_DATE.length() == 0)
					INITIAL_ACTIVATION_DATE_FLAG = "Y";
				else
					INITIAL_ACTIVATION_DATE_FLAG = "N";
				
				
				if(IsSubuscriberDiscarded(MSISDN, CCS_ACCT_TYPE_ID, SERVICE_STATE, INITIAL_ACTIVATION_DATE,INITIAL_ACTIVATION_DATE_FLAG))
				{
					boolean AccountProductIDFlag = false;
					List<String> offerList = new ArrayList<>();
					List<String> offerAttrList = new ArrayList<>();
					List<String> UCCounterList = new ArrayList<>();

					//Populate Subuscriber output				
					Subscriber subscriber = new Subscriber(subs , rejectAndLog, onlyLog ,INITIAL_ACTIVATION_DATE);
					subscriberListBuffer.addAll(subscriber.execute());
					
					//System.out.println("Going to Write Output for subuscriber: " + onlyLog.size());
					//Populate PAM and oFFEr output
					
					Offer offer = new Offer(subs, rejectAndLog, onlyLog, trackLog, INITIAL_ACTIVATION_DATE);
					Map<String, List<String>> offerMap = offer.execute();
					offerListBuffer.addAll(offerMap.get("Offer"));
					pamListBuffer.addAll(offerMap.get("Pam"));
					
					//System.out.println("Going to Write Output for offer: " + onlyLog.size());
					
					//Populate Offer Attribute output
					OfferAttribute offerattr = new OfferAttribute(subs, rejectAndLog, onlyLog,INITIAL_ACTIVATION_DATE);
					Map<String, List<String>> offerAttrMap = offerattr.execute();
					offerAttributeListBuffer.addAll(offerAttrMap.get("OfferAttribute"));
					
					//Populate DedicatedAccount output
					DedicatedAccount da = new DedicatedAccount(subs, rejectAndLog, onlyLog,trackLog, roundOffLog, INITIAL_ACTIVATION_DATE);
					//Map<String, String> daMap = (da.execute());
					Map<String, String> daMap = SortDABasedOnValue(da.execute());				
					
					
					//System.out.println("Going to Write Output for da: " + onlyLog.size());
					//Populate UsageCounter output
					UsageCounter uc = new UsageCounter(subs, rejectAndLog, onlyLog, trackLog);
					ucBuffer.addAll(uc.execute());
					
