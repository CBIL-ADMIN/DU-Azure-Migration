	public synchronized void businessLogicImplementation(String xml) throws JiBXException, IOException {
		long start = Calendar.getInstance().getTimeInMillis();
		//performanceLog.add("starting File at: " + LocalTime.now());
		{		
			try {
				
				//System.out.println(xml);
				//System.out.println("Start of transformation Thread: " + Thread.currentThread().getName() + " ThreadID: " + Thread.currentThread().getId());
				
				subs = (SubscriberXml) UnmarshallingContextSubuscriber.unmarshalDocument(new ByteArrayInputStream(xml.getBytes()), null);
				
				String MSISDN = subs.getSubscriberInfoMSISDN();
				uniqueBEMSISDN.add(MSISDN);
				String CCS_ACCT_TYPE_ID = subs.getSubscriberInfoCCSACCTTYPEID();
				String SERVICE_STATE = subs.getSubscriberInfoSERVICESTATE();
				//getting intial_activation_date form USMS input
				if(subs.getUsmsdumpInfoList().size() == 1 && subs.getUsmsdumpInfoList().get(0).getINITIALACTIVATIONDATE() != null)
				{
					this.INITIAL_ACTIVATION_DATE = subs.getUsmsdumpInfoList().get(0).getINITIALACTIVATIONDATE();
				}
				
				String INITIAL_ACTIVATION_DATE_FLAG;
				
				if(this.INITIAL_ACTIVATION_DATE.length() == 0)
					INITIAL_ACTIVATION_DATE_FLAG = "Y";
				else
					INITIAL_ACTIVATION_DATE_FLAG = "N";
				
				
				if(IsSubuscriberDiscarded(MSISDN, CCS_ACCT_TYPE_ID, SERVICE_STATE, INITIAL_ACTIVATION_DATE,INITIAL_ACTIVATION_DATE_FLAG))
				{
					boolean AccountProductIDFlag = false;
										
					ProductIDLookUpMap.clear();
					ProfileProductIDLookUpMap.clear();

					//Populate Subuscriber output				
					Subscriber subscriber = new Subscriber(subs , rejectAndLog, onlyLog ,INITIAL_ACTIVATION_DATE);
					subscriberListBuffer.addAll(subscriber.execute());
					
					//System.out.println("Going to Write Output for subuscriber: " + onlyLog.size());
					//Populate PAM and oFFEr output
					
					Offer offer = new Offer(subs, rejectAndLog, onlyLog, trackLog, INITIAL_ACTIVATION_DATE, ProductIDLookUpMap, ProductIDFor53, ProfileProductIDLookUpMap);
					Map<String, List<String>> offerMap = offer.execute();
					offerListBuffer.addAll(offerMap.get("Offer"));
					pamListBuffer.addAll(offerMap.get("Pam"));
					
					//System.out.println("Going to Write Output for offer: " + onlyLog.size());
					
					//Populate Offer Attribute output
					OfferAttribute offerattr = new OfferAttribute(subs, rejectAndLog, onlyLog,INITIAL_ACTIVATION_DATE, ProductIDLookUpMap);
					Map<String, List<String>> offerAttrMap = offerattr.execute();
					offerAttributeListBuffer.addAll(offerAttrMap.get("OfferAttribute"));
					
					//Populate DedicatedAccount output
					DedicatedAccount da = new DedicatedAccount(subs, rejectAndLog, onlyLog,trackLog, roundOffLog, INITIAL_ACTIVATION_DATE, ProductIDLookUpMap);
					//Map<String, String> daMap = (da.execute());
					Map<String, String> daMap = SortDABasedOnValue(da.execute());				
					
					
					//System.out.println("Going to Write Output for da: " + onlyLog.size());
					//Populate UsageCounter output
					UsageCounter uc = new UsageCounter(subs, rejectAndLog, onlyLog, trackLog, ProductIDLookUpMap, ProductIDFor53);
					ucBuffer.addAll(uc.execute());
					
					//from DA map populate the 63 columns for Dedicated acount
					List<String> daList = new ArrayList<>();
					
					//Fill the DA as per SDP server
					if(daMap.size() >= 1)
					{
						StringBuffer sb = new StringBuffer();
						StringBuffer sb2 = new StringBuffer();
						StringBuffer sb3 = new StringBuffer();
						StringBuffer sb4 = new StringBuffer();
						StringBuffer sb5 = new StringBuffer();
						StringBuffer sb6 = new StringBuffer();
						String[] splittedColumns = DACOLUMNS.split(",");
						int columnCount = 1;						
						if (daMap.containsKey("ID_1")) {
							sb.append(MSISDN).append(",");
							sb.append(1).append(",");
	
							for (String column : splittedColumns) {
								if (columnCount <= splittedColumns.length - 1) {
									if (daMap.containsKey(column)) {
										sb.append(daMap.get(column)).append(",");
									} else {
										if (column.startsWith("ID") || column.startsWith("BALANCE")) {
											sb.append("0,");
										} else {
											if (column.startsWith("PAM_SERVICE_ID")) {
											sb.append("0,");
											}
											else {
												if (column.startsWith("EXPIRY_DATE") || column.startsWith("START_DATE")) {
												sb.append("NULL,");
												}
												else
												{
												if (column.startsWith("PRODUCT_ID")) {
													sb.append("0,");
													}	
												}
											} 
										}
									}
								} else {
									if (daMap.containsKey(column)) {
										sb.append(daMap.get(column));
									} else {
	
										sb.append("NULL");
									}
								}
								if(columnCount == 60)
									break;
								columnCount++;								
							}														
						}
						if (daMap.containsKey("ID_1")) {
							daList.add(sb.toString());
						}
						sb = null;
						
						//it means we have more DA to be added so populate Sequence ID=2
						if(daMap.size() > 60)
						{
							String[] splittedColumns2 = DACOLUMNS2.split(",");
							int columnCount2 = 1;						
							if (daMap.containsKey("ID_11")) {
								sb2.append(MSISDN).append(",");
								sb2.append(2).append(",");
		
								for (String column : splittedColumns2) {
									if (columnCount2 <= splittedColumns2.length - 1) {
										if (daMap.containsKey(column)) {
											sb2.append(daMap.get(column)).append(",");
										} else {
											if (column.startsWith("ID") || column.startsWith("BALANCE")) {
												sb2.append("0,");
											} else {
												if (column.startsWith("PAM_SERVICE_ID")) {
													sb2.append("0,");
												}
												else {
													if (column.startsWith("EXPIRY_DATE") || column.startsWith("START_DATE")) {
														sb2.append("NULL,");
													}
													else
													{
													if (column.startsWith("PRODUCT_ID")) {
														sb2.append("0,");
														}	
													}
												} 
											}
										}
									} else {
										if (daMap.containsKey(column)) {
											sb2.append(daMap.get(column));
										} else {
		
											sb2.append("NULL");
										}
									}
									columnCount2++;										
								}			
