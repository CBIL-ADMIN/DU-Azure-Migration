	public ExecuteTransformation()
	{
		
	}
	
	public ExecuteTransformation(String pathOfOutputFolder, final String pathtoApplicationContext, String pathOfLogFolder) {
		this.pathOfOutputFolder = pathOfOutputFolder;
		this.pathOfLogFolder = "/" + pathOfLogFolder;

		this.pathtoApplicationContext = pathtoApplicationContext;
		accountListBuffer = new ArrayList<>();
		subscriberListBuffer = new ArrayList<>();
		offerListBuffer = new ArrayList<>();
		offerAttributeListBuffer = new ArrayList<>();
		pamListBuffer = new ArrayList<>();
		ucBuffer = new ArrayList<>();
		daBuffer = new ArrayList<>();
		cisOnceOff = new ArrayList<>();
		cisRenewal = new ArrayList<>();
		cisParking = new ArrayList<>();
		cisNwpc = new ArrayList<>();
		subscriberFaFBuffer = new ArrayList<>();
		servicenumberBuffer = new ArrayList<>();
		accountListBuffer = new ArrayList<>();
		accumulatorBuffer= new ArrayList<>();
		INITIAL_ACTIVATION_DATE = "";
		uniqueBEMSISDN = Collections.synchronizedSet(new HashSet<String>());
		
		//SortedBalanceInput = new CopyOnWriteArrayList<>(); //ArrayList<>();
		
		rejectAndLog = Collections.synchronizedSet(new HashSet<String>());
		onlyLog = Collections.synchronizedSet(new HashSet<String>());
		roundOffLog = Collections.synchronizedSet(new HashSet<String>());
		trackLog = Collections.synchronizedSet(new HashSet<String>());
		CISonlyLog = Collections.synchronizedSet(new HashSet<String>());
		performanceLog = Collections.synchronizedSet(new HashSet<String>());
		ProductIDLookUpMap = Collections.synchronizedMap(new ConcurrentHashMap<>(10000, 0.75f, 100));
		ProfileProductIDLookUpMap = Collections.synchronizedMap(new ConcurrentHashMap<>(10000, 0.75f, 100));
		ProductIDFor53 = Collections.synchronizedMap(new ConcurrentHashMap<>(10000, 0.75f, 100));
		try {

			bindingFactorySubuscriber = BindingDirectory.getFactory(SubscriberXml.class);
			UnmarshallingContextSubuscriber = bindingFactorySubuscriber.createUnmarshallingContext();
	

			// Generate random integers in range 0 to 999999
			uniqueNumber = LoadSubscriberMapping.rand.nextInt(1000000);

		} catch (JiBXException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
			LOG.error("JIBX Exception ", e); 
		}

	}

	private boolean IsSubuscriberDiscarded(String MSISDN, String CCS_ACCT_TYPE_NAME, String SERVICE_STATE, String INITIAL_ACTIVATION_DATE, String INITIAL_ACTIVATION_DATE_FLAG)
	{
		boolean IsServiceClassDiscarded = false;
		boolean IsLifecycleNotDiscarded = false;
		
		if(LoadSubscriberMapping.ServiceClassMap.containsKey(CCS_ACCT_TYPE_NAME))
		{
			String Value = LoadSubscriberMapping.ServiceClassMap.get(CCS_ACCT_TYPE_NAME);
			if(Value.split("\\|")[1].equals("N"))
			{
				IsServiceClassDiscarded = true;
			}
			else
			{
				if(Arrays.stream(LoadSubscriberMapping.CommonConfigMap.get("SNA_Rate_Plan_ID_Identifier").split("\\|")).anyMatch(CCS_ACCT_TYPE_NAME::equals))
				{
					if((subs.getUsmsdumpInfoList().size() == 0) || (subs.getProfiledumpInfoList().size() == 0))
					{
						rejectAndLog.add("INC9002:SNA Destination number is empty:MSISDN=" + MSISDN + ":CCS_ACCT_TYPE_ID="+CCS_ACCT_TYPE_NAME+ ":ACTION=Discard and log");
						return false;
					}
					else
					{
						ServiceNumberApplication sna = new ServiceNumberApplication(subs,rejectAndLog, onlyLog);
						rejectAndLog.add("INC1002:Service class Ignored:MSISDN=" + MSISDN + ":CCS_ACCT_TYPE_ID="+CCS_ACCT_TYPE_NAME+ ":SERVICE_STATE=" + SERVICE_STATE + ":ACTION=Discard and log");
						servicenumberBuffer.addAll(sna.execute());
						return false;
					}
				}
				
				rejectAndLog.add("INC1002:Service class Ignored:MSISDN=" + MSISDN + ":CCS_ACCT_TYPE_ID="+CCS_ACCT_TYPE_NAME+ ":SERVICE_STATE=" + SERVICE_STATE + ":ACTION=Discard and log");
				return false;
			}
			IsServiceClassDiscarded = true;
		}
		else
		{
			rejectAndLog.add("INC1001:Service class Lookup Failed:MSISDN=" + MSISDN + ":CCS_ACCT_TYPE_ID="+CCS_ACCT_TYPE_NAME+ ":SERVICE_STATE=" + SERVICE_STATE + ":ACTION=Discard and log");
			return false;
		}
		
		if (IsServiceClassDiscarded)
		{
			if(INITIAL_ACTIVATION_DATE_FLAG.equals("N"))
			{
				String[] values = {"A","S","F","D","T"};
				if (Arrays.stream(values).anyMatch(SERVICE_STATE::equals))
				{
					IsLifecycleNotDiscarded = true;
				}
				else
				{
					rejectAndLog.add("INC1004:Lifecycle lookup mismatch:MSISDN=" + MSISDN + ":SERVICE_STATE=" + SERVICE_STATE + ":ACTION=Discard and log");	
					return false;
				}
				
			}
			else if(INITIAL_ACTIVATION_DATE_FLAG.equals("Y"))
			{
				String[] values = {"P","S"};
				if (Arrays.stream(values).anyMatch(SERVICE_STATE::equals))
				{
					IsLifecycleNotDiscarded = true;					
				}
				else
				{
					rejectAndLog.add("INC1004:Lifecycle lookup mismatch:MSISDN=" + MSISDN + ":SERVICE_STATE=" + SERVICE_STATE + ":ACTION=Discard and log");
					return false;
				}
			}
			if(IsLifecycleNotDiscarded)
			{
				if((subs.getUsmsdumpInfoList().size() == 0) || (subs.getProfiledumpInfoList().size() == 0))
				{
					//rejectAndLog.add("INC1003:Subscriber relation data missing:MSISDN=" + MSISDN + ":ACTION=Discard and log");
					return false;
				}
				else
				{
					IsLifecycleNotDiscarded = true;
				}
			}
		}
		return IsLifecycleNotDiscarded;
	}
	
