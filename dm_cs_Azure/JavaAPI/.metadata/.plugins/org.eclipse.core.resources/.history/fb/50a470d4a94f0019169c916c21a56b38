	public synchronized void businessLogicImplementation(String xml) throws JiBXException, IOException {
		//synchronized (ThreadLock)
		{		
			try {
				
				//System.out.println(xml);
				//System.out.println("Start of transformation Thread: " + Thread.currentThread().getName() + " ThreadID: " + Thread.currentThread().getId());
				
				subs = (SubscriberXml) UnmarshallingContextSubuscriber.unmarshalDocument(new ByteArrayInputStream(xml.getBytes()), null);
				
				String MSISDN = subs.getSubscriberInfoMSISDN();
				String CCS_ACCT_TYPE_ID = subs.getSubscriberInfoCCSACCTTYPEID();
				String SERVICE_STATE = subs.getSubscriberInfoSERVICESTATE();
				//getting intial_activation_date form USMS input
				if(subs.getUsmsdumpInfoList().size() == 1 && subs.getUsmsdumpInfoList().get(0).getINITIALACTIVATIONDATE() != null)
				{
					this.INITIAL_ACTIVATION_DATE = subs.getUsmsdumpInfoList().get(0).getINITIALACTIVATIONDATE();
				}
				
				String INITIAL_ACTIVATION_DATE_FLAG;
				
				if(this.INITIAL_ACTIVATION_DATE.length() == 0)
					INITIAL_ACTIVATION_DATE_FLAG = "Y";
				else
					INITIAL_ACTIVATION_DATE_FLAG = "N";
				
				
				if(IsSubuscriberDiscarded(MSISDN, CCS_ACCT_TYPE_ID, SERVICE_STATE, INITIAL_ACTIVATION_DATE,INITIAL_ACTIVATION_DATE_FLAG))
				{
					boolean AccountProductIDFlag = false;
					List<String> offerList = new ArrayList<>();
					List<String> offerAttrList = new ArrayList<>();
					List<String> UCCounterList = new ArrayList<>();

					//Populate Subuscriber output				
					Subscriber subscriber = new Subscriber(subs , rejectAndLog, onlyLog ,INITIAL_ACTIVATION_DATE);
					subscriberListBuffer.addAll(subscriber.execute());
					
					//System.out.println("Going to Write Output for subuscriber: " + onlyLog.size());
					//Populate PAM and oFFEr output
					
					Offer offer = new Offer(subs, rejectAndLog, onlyLog, trackLog, INITIAL_ACTIVATION_DATE);
					Map<String, List<String>> offerMap = offer.execute();
					offerList.addAll(offerMap.get("Offer"));
					pamListBuffer.addAll(offerMap.get("Pam"));
					
					//System.out.println("Going to Write Output for offer: " + onlyLog.size());
					
					//Populate Offer Attribute output
					OfferAttribute offerattr = new OfferAttribute(subs, rejectAndLog, onlyLog,INITIAL_ACTIVATION_DATE);
					Map<String, List<String>> offerAttrMap = offerattr.execute();
					offerAttrList.addAll(offerAttrMap.get("OfferAttribute"));
					
					//Populate DedicatedAccount output
					DedicatedAccount da = new DedicatedAccount(subs, rejectAndLog, onlyLog,trackLog, roundOffLog, INITIAL_ACTIVATION_DATE);
					//Map<String, String> daMap = (da.execute());
					Map<String, String> daMap = SortDABasedOnValue(da.execute());				
					
					
					//System.out.println("Going to Write Output for da: " + onlyLog.size());
					//Populate UsageCounter output
					UsageCounter uc = new UsageCounter(subs, rejectAndLog, onlyLog, trackLog);
					UCCounterList.addAll(uc.execute());
					uctempBuffer.addAll(UCCounterList);
					//Creating a list and then updating it to make sure no duplicates.
					offerAttributeListBuffer.addAll(offerAttrList);
					
					//System.out.println("Going to Write Output for uc: " + onlyLog.size());				
					//Polulate the ProductID to DA,offer and UC
					Set<String> UCCompleted = new HashSet<>();
					int ProductId = 100;
					for(String item : offerList)
					{
						String OfferID = item.split(",")[1];
						
						int CompleteGroup = 0;
						if(LoadSubscriberMapping.BalanceProductID.containsKey(OfferID))
						{
							//System.out.println(item);
							String value = item.replace("toBePopulated",String.valueOf(ProductId));
							offerListBuffer.add(value);
							
							Map<String, List<String>> ProductIDUADA = LoadSubscriberMapping.BalanceProductID.get(OfferID);
							List<String>DA_ProductID = ProductIDUADA.get("DAValue");
							List<String>UC_ProductID = ProductIDUADA.get("UCValue");
							List<String>OffAttr_ProductID = ProductIDUADA.get("OffAttrValue");
							
							if(DA_ProductID.size() > 0)
							{
								for(Map.Entry<String,String> entry: daMap.entrySet())
								{
									//System.out.println(entry.getValue());
									if(entry.getKey().toString().startsWith("ID_"))
									{
										String ID_Number = entry.getKey().toString().split("_")[1];
										if(DA_ProductID.contains(entry.getValue().toString()) && daMap.get("PRODUCT_ID_"+ID_Number).equals("0"))
										{
											daMap.put("PRODUCT_ID_"+ID_Number,String.valueOf(ProductId));
											break;
										}
									}
								}
							}	
							if(OffAttr_ProductID.size() > 0)
							{
								for(String offAttr :offerAttrList)
								{
									String Attr_ID = offAttr.split(",")[1];
									if(OffAttr_ProductID.contains(Attr_ID))
									{
										offerAttributeListBuffer.remove(offAttr);
										String temp = offAttr.substring(0, offAttr.lastIndexOf(","));
										
										offerAttributeListBuffer.add(temp+ "," + ProductId );									
									}
								}
							}
							
							if(UC_ProductID.size() > 0)
							{																	
								if(CheckIfUCGroupPresent(UC_ProductID))
								{
									for(String counter :UCCounterList)
									{
										String CounterID = counter.split(",")[1];
										
										if(UC_ProductID.contains(CounterID))
										{
											
											for(Iterator<String> i = uctempBuffer.iterator(); i.hasNext();)
											{
											    if (i.next().equals(counter))
											    {
											    	CompleteGroup++;
											        i.remove();
											        ucBuffer.add(MSISDN + "," + CounterID + "," + counter.split(",")[2]+ "," + counter.split(",")[3]
															+ "," + ProductId + "," + counter.split(",")[5] + "," + counter.split(",")[6]);
													
											        break;
											    }
											    else if(uctempBuffer.size() == 1 && uctempBuffer.contains(counter) )
											    {
											    	i.remove();
											    	CompleteGroup++;
											    	ucBuffer.add(MSISDN + "," + CounterID + "," + counter.split(",")[2]+ "," + counter.split(",")[3]
															+ "," + ProductId + "," + counter.split(",")[5] + "," + counter.split(",")[6]);
													
											    	break;
											    }
											}
											if(CompleteGroup == UC_ProductID.size())
												break;
										}
									}
								}
								else
								{
									for(String counter :UCCounterList)
									{
										String CounterID = counter.split(",")[1];
										if(UC_ProductID.contains(CounterID)) // && !UCCompleted.contains(counter))
										{
											UCCompleted.add(counter);
											//ucBuffer.remove(counter);
											for(Iterator<String> i = uctempBuffer.iterator(); i.hasNext();)
											{
											    if (i.next().equals(counter))
											    {
											        i.remove();
											        ucBuffer.add(MSISDN + "," + CounterID + "," + counter.split(",")[2]+ "," + counter.split(",")[3]
															+ "," + ProductId + "," + counter.split(",")[5] + "," + counter.split(",")[6]);
													
											        break;
											    }
											    else if(uctempBuffer.size() == 1 && uctempBuffer.contains(counter) )
											    {
											    	i.remove();
											    	ucBuffer.add(MSISDN + "," + CounterID + "," + counter.split(",")[2]+ "," + counter.split(",")[3]
															+ "," + ProductId + "," + counter.split(",")[5] + "," + counter.split(",")[6]);
													
											    	break;
											    }
											}
											break;
										}								
									}
								}	
							}							
							AccountProductIDFlag = true;
							ProductId ++;
						}
						else
						{
							String value = item.replace("toBePopulated",LoadSubscriberMapping.CommonConfigMap.get("default_ZERO"));
							offerListBuffer.add(value);
							
							//update its UC as well
							/*for(String counter :tempUCList)
							{
								String CounterID = counter.split(",")[1];
								if(CounterID.contains(OfferID))
								{
									ucFinalBuffer.add(counter);
									break;
								}
							}*/
						}
					}		
					
					ucBuffer.addAll(uctempBuffer);
					
					//from DA map populate the 63 columns for Dedicated acoount
					List<String> daList = new ArrayList<>();
					
					//Fill the DA as per SDP server
					if(daMap.size() >= 1)
					{
						StringBuffer sb = new StringBuffer();
						StringBuffer sb2 = new StringBuffer();
						String[] splittedColumns = DACOLUMNS.split(",");
						int columnCount = 1;						
						if (daMap.containsKey("ID_1")) {
							sb.append(MSISDN).append(",");
							sb.append(1).append(",");
	
							for (String column : splittedColumns) {
								if (columnCount <= splittedColumns.length - 1) {
									if (daMap.containsKey(column)) {
										sb.append(daMap.get(column)).append(",");
									} else {
										if (column.startsWith("ID") || column.startsWith("BALANCE")) {
											sb.append("0,");
										} else {
											if (column.startsWith("PAM_SERVICE_ID")) {
											sb.append("0,");
											}
											else {
												if (column.startsWith("EXPIRY_DATE") || column.startsWith("START_DATE")) {
												sb.append("NULL,");
												}
												else
												{
												if (column.startsWith("PRODUCT_ID")) {
													sb.append("0,");
													}	
												}
											} 
										}
									}
								} else {
									if (daMap.containsKey(column)) {
										sb.append(daMap.get(column));
									} else {
	
										sb.append("NULL");
									}
								}
								if(columnCount == 60)
									break;
								columnCount++;								
							}														
						}
						if (daMap.containsKey("ID_1")) {
							daList.add(sb.toString());
						}
						sb = null;
						
						//it means we have more DA to be added so populate Sequence ID=2
						if(daMap.size() > 60)
						{
							String[] splittedColumns2 = DACOLUMNS2.split(",");
							int columnCount2 = 1;						
							if (daMap.containsKey("ID_11")) {
								sb2.append(MSISDN).append(",");
								sb2.append(2).append(",");
		
								for (String column : splittedColumns2) {
									if (columnCount2 <= splittedColumns2.length - 1) {
										if (daMap.containsKey(column)) {
											sb2.append(daMap.get(column)).append(",");
										} else {
											if (column.startsWith("ID") || column.startsWith("BALANCE")) {
												sb2.append("0,");
											} else {
												if (column.startsWith("PAM_SERVICE_ID")) {
													sb2.append("0,");
												}
												else {
													if (column.startsWith("EXPIRY_DATE") || column.startsWith("START_DATE")) {
														sb2.append("NULL,");
													}
													else
													{
													if (column.startsWith("PRODUCT_ID")) {
														sb2.append("0,");
														}	
													}
												} 
											}
										}
									} else {
										if (daMap.containsKey(column)) {
											sb2.append(daMap.get(column));
										} else {
		
											sb2.append("NULL");
										}
									}
									columnCount2++;										
								}			
							}
						}
						if (daMap.containsKey("ID_11")) {
							daList.add(sb2.toString());
						}
					}
					
					daBuffer.addAll(daList);
					
					Account account = new Account(subs,rejectAndLog, onlyLog,INITIAL_ACTIVATION_DATE,AccountProductIDFlag);
					accountListBuffer.addAll(account.execute());
					
					//System.out.println("Going to Write Output for account: " + onlyLog.size());
					//Populate SNA output
					Accumulator ac = new Accumulator(subs, rejectAndLog, onlyLog,INITIAL_ACTIVATION_DATE);
					Map<String, String> accMap = ac.execute();
					
					List<String> uaList = new ArrayList<>();
					
					if(accMap.size() >= 1)
					{
						StringBuffer sb = new StringBuffer();
						StringBuffer sb2 = new StringBuffer();
						String[] splittedColumns = ACMCOLUMNS.split(",");
						int columnCount = 1;
						if (accMap.containsKey("ID_1")) {
							sb.append(MSISDN).append(",");
							sb.append(1).append(",");
	
							for (String column : splittedColumns) {
								if (columnCount <= splittedColumns.length-1) {
									if (accMap.containsKey(column)) {
										sb.append(accMap.get(column)).append(",");
									} else {
										if (column.startsWith("ID") || column.startsWith("VALUE")) {
											sb.append("0,");
										} else {
											if (column.startsWith("CLEARING_DATE")) {
											sb.append(LoadSubscriberMapping.CommonConfigMap.get("default_last_reset_Date") +",");
											}										
										}
									}
								} else {
									if (accMap.containsKey(column)) {
										sb.append(accMap.get(column));
									} else {
	
										sb.append(LoadSubscriberMapping.CommonConfigMap.get("default_last_reset_Date"));
									}
								}
								columnCount++;
							}
						}
						if (accMap.containsKey("ID_1")) {
							uaList.add(sb.toString());
						}
						sb = null;
						//it means we have more DA to be added so populate Sequence ID=2
						if(accMap.size() > 30)
						{
							String[] splittedColumns2 = ACMCOLUMNS2.split(",");
							int columnCount2 = 1;
							if (accMap.containsKey("ID_1")) {
								sb2.append(MSISDN).append(",");
								sb2.append(2).append(",");
		
								for (String column : splittedColumns2) {
									if (columnCount2 <= splittedColumns2.length-1) {
										if (accMap.containsKey(column)) {
											sb2.append(accMap.get(column)).append(",");
										} else {
											if (column.startsWith("ID") || column.startsWith("VALUE")) {
												sb2.append("0,");
											} else {
												if (column.startsWith("CLEARING_DATE")) {
													sb2.append(LoadSubscriberMapping.CommonConfigMap.get("default_last_reset_Date") +",");
												}										
											}
										}
									} else {
										if (accMap.containsKey(column)) {
											sb2.append(accMap.get(column));
										} else {		
											sb2.append(LoadSubscriberMapping.CommonConfigMap.get("default_last_reset_Date"));
										}
									}
									columnCount2++;
								}
							}
							if (accMap.containsKey("ID_1")) {
								uaList.add(sb2.toString());
							}
							sb2 = null;
						}						
						
					}
					
					accumulatorBuffer.addAll(uaList);
					
					//System.out.println("Going to Write Output for Accumulator: " + onlyLog.size());
					//Populate CIS output
					CISOutputs cis = new CISOutputs(subs, rejectAndLog, onlyLog,INITIAL_ACTIVATION_DATE);
					Map<String, List<String>> CISMap = cis.execute();
					cisOnceOff.addAll(CISMap.get("CIS_OnceOff_Bundles"));
					cisRenewal.addAll(CISMap.get("CIS_Renewal_Bundles"));
					cisParking.addAll(CISMap.get("CIS_Parking_Bundles"));
					cisNwpc.addAll(CISMap.get("CIS_nwpc_Bundles"));
					
					//Populate SNA output
					/*ServiceNumberApplication sna = new ServiceNumberApplication(subs,rejectAndLog, onlyLog);
					servicenumberBuffer.addAll(sna.execute());*/
					
					//Populate subuscriberFaf output
					SubscriberFaf subsfaf = new SubscriberFaf(subs, rejectAndLog, onlyLog);
					subscriberFaFBuffer.addAll(subsfaf.execute());
					
					//System.out.println("Going to Write Output for FAF: " + onlyLog.size());					
					//onlyLog.forEach(action->{System.out.println(action);});
					
					account = null;
					subscriber = null;
					offer = null;
					subs = null;
					da = null;
					uc = null;
					ac = null;
					uctempBuffer.clear();
					
					//System.out.println("**********Size of Balanace input: " + SortedBalanceInput.size());
					//SortedBalanceInput.clear();
					//System.out.println("**********Size of Balanace input: " + SortedBalanceInput.size());
				}
	
			} catch (JiBXException e) {
				// TODO Auto-generated catch block
				e.printStackTrace();
				LOG.error("JIBX Exception ", e);
				LOG.error(xml);
				// System.out.println(xml);
			}
		}
		// return fulllist;
	}
	
	private boolean CheckIfUCGroupPresent(List<String> ucList) {
		// TODO Auto-generated method stub
		boolean GroupPresent = false;
		int counter = 0;
		for(String uc : ucList)
		{
			for(String val : uctempBuffer)
			{
				String CounterID = val.split(",")[1];
				if(CounterID.equals(uc))
				{
					counter++;
					GroupPresent = false;
					break;
				}				
			}
		}
		if(counter >= 2)
			return true;
		
		return GroupPresent;
	}

	private Map<String, String> SortDABasedOnValue(Map<String, String> unsortedDA) {
		Map<String,String> sortedDA = new ConcurrentHashMap<>();
		
		int index = 1;
		List<String> sDA_Value = unsortedDA.entrySet().stream().filter(e -> e.getKey().startsWith("ID_"))
				.map(Map.Entry::getValue).collect(Collectors.toList());
		
		List<Integer> DA_Value = sDA_Value.stream().map(Integer::parseInt).collect(Collectors.toList());
		
		Collections.sort(DA_Value);
		
		Set<String> CompletedDAID = new HashSet<>();
		for(int DAID : DA_Value)
		{
			for(Map.Entry<String, String> entry : unsortedDA.entrySet())
			{				
				if(entry.getKey().startsWith("ID_") && DAID == Integer.parseInt(entry.getValue()) && !CompletedDAID.contains(entry.getKey()))
				{
					String IDName = entry.getKey().split("_")[1];
					
					sortedDA.put("ID_" + index, String.valueOf(DAID));
					sortedDA.put("BALANCE_" + index, unsortedDA.get("BALANCE_" + IDName));
					sortedDA.put("START_DATE_" + index,unsortedDA.get("START_DATE_" + IDName));
					sortedDA.put("EXPIRY_DATE_" + index,unsortedDA.get("EXPIRY_DATE_" + IDName));
					sortedDA.put("PAM_SERVICE_ID_" + index,unsortedDA.get("PAM_SERVICE_ID_" + IDName));
					sortedDA.put("PRODUCT_ID_" + index, unsortedDA.get("PRODUCT_ID_" + IDName));
					index++;
					CompletedDAID.add(entry.getKey());
					break;
				}
			}
		}
				
		
		return sortedDA;
	}

