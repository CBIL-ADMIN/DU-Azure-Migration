        <class_source>int counter = 0;
String kettleHome = "";
public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException {
  if (first) {
    first = false;
  }

  Object[] r = getRow();

  if (r == null) {
    setOutputDone();
    return false;
  }

  // It is always safest to call createOutputRow() to ensure that your output row's Object[] is large
  // enough to handle any new fields you are creating in this step.
  r = createOutputRow(r, data.outputRowMeta.size());

  // Send the row on to the next step.
  String SubsFilePath = get(Fields.In, "SubsFilePath").getString(r);

  String FILE_NAME = "";
 
  String Result = "";  

  counter ++;
  if( counter == 1)
  {
	Result = "one";
	//FILE_NAME = kettleHome + "\\working\\" + SubsFilePath.substring(SubsFilePath.lastIndexOf("\\") +1,SubsFilePath.length()).replace("dump", "dump_01").replace(".gz", "");
	FILE_NAME = SubsFilePath.substring(SubsFilePath.lastIndexOf("\\") +1,SubsFilePath.length()).replace("dump", "dump_01").replace(".gz", "");
  }
  if( counter == 2)
  {
	Result = "two";
    //FILE_NAME = kettleHome + "\\working\\"  + SubsFilePath.substring(SubsFilePath.lastIndexOf("\\") +1,SubsFilePath.length()).replace("dump", "dump_02").replace(".gz", "");
	FILE_NAME = SubsFilePath.substring(SubsFilePath.lastIndexOf("\\") +1,SubsFilePath.length()).replace("dump", "dump_02").replace(".gz", "");
  }
  if( counter == 3)
  {
	Result = "three";
    //FILE_NAME = kettleHome + "\\working\\" + SubsFilePath.substring(SubsFilePath.lastIndexOf("\\") +1,SubsFilePath.length()).replace("dump", "dump_03").replace(".gz", "");
	FILE_NAME = SubsFilePath.substring(SubsFilePath.lastIndexOf("\\") +1,SubsFilePath.length()).replace("dump", "dump_03").replace(".gz", "");
  }
  if( counter == 4)
  {
	Result = "four";
    //FILE_NAME = kettleHome + "\\working\\" + SubsFilePath.substring(SubsFilePath.lastIndexOf("\\") +1,SubsFilePath.length()).replace("dump", "dump_04").replace(".gz", "");
	FILE_NAME = SubsFilePath.substring(SubsFilePath.lastIndexOf("\\") +1,SubsFilePath.length()).replace("dump", "dump_04").replace(".gz", "");
    counter = 0;
  }
  
  get(Fields.Out, "FILE_NAME").setValue(r, FILE_NAME);
  get(Fields.Out, "Result").setValue(r, Result);
  putRow(data.outputRowMeta, r);

  return true;
}

public boolean init(StepMetaInterface stepMetaInterface, StepDataInterface stepDataInterface) {
  kettleHome=getVariable("KETTLE_HOME","");
  return parent.initImpl(stepMetaInterface, stepDataInterface);
}

