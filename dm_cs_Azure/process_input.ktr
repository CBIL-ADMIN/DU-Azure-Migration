        <class_source>import java.util.*;
import javax.xml.transform.Source;
import javax.xml.transform.Transformer;
import javax.xml.transform.TransformerFactory;
import javax.xml.transform.stream.StreamResult;
import javax.xml.transform.stream.StreamSource;
import javax.xml.transform.TransformerConfigurationException;
import javax.xml.transform.TransformerException;
import java.io.StringReader;
import java.io.StringWriter;
import dm.saxon.message.MessageList;
import dm.saxon.message.DMMessageReceiver;


private Transformer transformer = null;
private HashMap accountClassMap;

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException {
	
	Object[] r=getRow();
	if (r==null)   {
	    setOutputDone();
		return false;
	}


  	if (first) {
     first=false;
  	}
	r = createOutputRow(r, data.outputRowMeta.size());
	
	
	
	String subscriber_xml = get(Fields.In, "subscriber_xml").getString(r);
	Source streamSource = new StreamSource(new StringReader(subscriber_xml));
	StreamResult resultTarget = new StreamResult(new StringWriter());
		
	//logBasic("subscriber_xml value is: " + subscriber_xml);

	MessageList ml= new MessageList();
	((net.sf.saxon.Controller)transformer).setMessageEmitter(new DMMessageReceiver(ml));
			
	try {
		transformer.transform(streamSource, resultTarget);
	} catch (TransformerConfigurationException e) {
		logError("xslt conf:"+e.toString()+"\n"+subscriber_xml);
	} catch (TransformerException e) {
		if (!e.getMessage().contains("Processing terminated by xsl:message"))
			logError("xslt transt:"+e.toString()+"\n"+subscriber_xml+"\n");
	}

		
	
	boolean fatalIncidence=ml.hasTerminate();
	int nb = ml.getNumberOfMessages();
    for (int i=0;i&lt;nb;i++) {
       	//System.err.println("Message(terminate="+ml.getTerminate(i)+")>>" + ml.getMessage(i));
		// putError is declared as follows:
       	// public void putError(RowMetaInterface rowMeta, Object[] row, long nrErrors, String errorDescriptions, String fieldNames, String errorCodes)
			
		//ESAMYUR: Currently, field and code are not used, it will be inside the message. To be studied for improvement.
		//logBasic(ml.getMessageContent(i));
       	putError(data.outputRowMeta, r, 1, ml.getMessageContent(i), "", "");
       
    }

	//if one of the xsl:message had terminate=yes, do not output normal row
	if (ml.hasTerminate()) 
       	// get on with the next line
		return true;

	Object[] outputRowData = RowDataUtil.resizeArray(r, data.outputRowMeta.size());
  	int outputIndex = getInputRowMeta().size();
  	outputRowData[outputIndex++] = resultTarget.getWriter().toString();

	putRow(data.outputRowMeta, outputRowData);

	return true;
}



public boolean init(StepMetaInterface stepMetaInterface, StepDataInterface stepDataInterface) {
   if (super.init(stepMetaInterface, stepDataInterface)) {
	try {

		// Force Java SDK to use Xalan to implement TransformerFactory.
		String key = "javax.xml.transform.TransformerFactory";
		String value = "net.sf.saxon.TransformerFactoryImpl";
		Properties props = System.getProperties();
		props.put(key, value);
		System.setProperties(props);
		

		logBasic("xsltSource: " + getVariable("KETTLE_HOME")+"/data/"+getVariable("XSLT_FILE"));
		Source xsltSource = new StreamSource(getVariable("KETTLE_HOME")+"/data/"+getVariable("XSLT_FILE"));
		
		TransformerFactory tFactory = TransformerFactory.newInstance();
		transformer = tFactory.newTransformer(xsltSource);

		} catch (TransformerConfigurationException e) {
			logError("XSLT initialization error:"+e.toString());
			e.printStackTrace();
			return false;
		} catch (KettleFileException e) {
			logError("XSLT file not found");
			e.printStackTrace();
			return false;
		} 
			catch (KettleStepException e) {
			logError("XSLT file not found");
			e.printStackTrace();
			return false;
		} 
			catch (Exception e) {
			logError(e.getMessage());
			e.printStackTrace();
			return false;
		} 

     return true;
   }
   return false;
}
