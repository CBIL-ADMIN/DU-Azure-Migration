    <definitions>
      <definition>
        <class_type>TRANSFORM_CLASS</class_type>
        <class_name>Processor</class_name>
        <class_source>import java.text.ParsePosition;
import java.text.SimpleDateFormat;
import java.util.Date;

private String Sub_State;


public boolean processRow(StepMetaInterface smi, StepDataInterface sdi)
throws KettleException
{
	if (first){
	  first = false;
	}
	Object[] r=getRow();

    if (r == null) {
      setOutputDone();
      return false;
    }
	r = createOutputRow(r, data.outputRowMeta.size());
	
	try
	{	
		
		//P (PreUse) - If Activation Date is empty 
		//A (Active) - If Activation Date is present and Supervision Period Expiry Date>=sysdate.
		//S (Suspend) - If Offer_ID 553 attached on subscriber and supervision date >= Current Date.
		//F (Frozen) - If Offer_ID 552 attached on subscriber and supervision date >= Current Date.
		//D (Dormant) - If Offer_ID 554 attached on subscriber and supervision date >= Current Date.
		//T (Terminated) - If Account Disconnection Date is &lt; than today.
		String Account_ID = get(Fields.In, "Account_ID").getString(r);
        String In_Account_Activated_Flag = get(Fields.In, "Account_Activated_Flag").getString(r);	
		String In_Account_Activated_Date = get(Fields.In, "Account_Activated_Date").getString(r);			
		String In_SFee_Exp_Date = get(Fields.In, "Service_Fee_Expiry_Date").getString(r);
		String In_Credit_Clearance_Date = get(Fields.In, "Credit_Clearance_Date").getString(r);
		String In_Sup_Period_Exp_Date = get(Fields.In, "Supervision_Period_Expiry_Date").getString(r);
		String In_Acc_Disconnection_Date = get(Fields.In, "Account_Disconnection_Date").getString(r);
		Long Offer = get(Fields.In, "Offer_ID").getInteger(r);
		String Temporary_Block_Flag = get(Fields.In, "Temporary_Block_Flag").getString(r);

		//logBasic("----" +Account_ID);
		//logBasic(Temporary_Block_Flag);
		if(Temporary_Block_Flag == "1")
		{
			Sub_State	=	"SUSPEND";
		}
		else
		{
			//logBasic(In_Account_Activated_Date);
			if(In_Account_Activated_Date == null)
			{
				Sub_State	=	"PREUSE";	
			}
			else
			{
				if(Offer != null)
				{				
					if (Offer == 553)
					{
						Sub_State = "SUSPEND";
					}	
					else if (Offer == 552)
					{
						Sub_State = "FROZEN";
					}
					else if (Offer == 554)
					{
						Sub_State = "DORMANT";
					}
					else
					{
						if (checkSubscriberTermination(In_Acc_Disconnection_Date))
						{
							Sub_State="TERMINATED";
						}
						else
						{
							Sub_State="ACTIVE";
						}
					}
				}
				else
				{
					if (checkSubscriberTermination(In_Acc_Disconnection_Date))
					{
						Sub_State="TERMINATED";
					}
					else
					{
						Sub_State="ACTIVE";
					
					}
				}
			}
		}

		logBasic("Value of Sub_State: " + Sub_State);
		
		get(Fields.Out, "Sub_State").setValue(r, Sub_State);

		r = createOutputRow(r, data.outputRowMeta.size());
		
		putRow(data.outputRowMeta, r);
	
	}
	catch(Exception exception)
	{
		exception.printStackTrace();
		logError(exception.getMessage(), exception);
	}
	finally
	{

	}
	return true;
}



private boolean checkSubscriberTermination(String In_Acc_Disconnection_Date)
	{
		String DT_YYYY_MM_DD="yyyy-MM-dd";
		Date Acc_Disconnection=null;
		
		try
		{

			if (In_Acc_Disconnection_Date!=null &amp;&amp; In_Acc_Disconnection_Date.trim().length()>0)	
				Acc_Disconnection=convertToDate(In_Acc_Disconnection_Date, DT_YYYY_MM_DD);

			
			//Active - If Activation Date is present and Supervision Period Expiry Date>=sysdate
			
			
			Date date					= new Date();
			String migDate				= new SimpleDateFormat("yyyy-MM-dd").format(date);
			ParsePosition porpos1=new ParsePosition(0);
			SimpleDateFormat sdf1= new SimpleDateFormat("yyyy-MM-dd");
			Date curr_date = sdf1.parse(migDate,porpos1);

		
			if (Acc_Disconnection.compareTo(curr_date)&lt;0)
				return true;
			
						
		}
		catch(Exception exception)
		{
			exception.printStackTrace();
		}
		return false;
}


public boolean init(StepMetaInterface stepMetaInterface, StepDataInterface stepDataInterface)
{
    return parent.initImpl(stepMetaInterface, stepDataInterface);
}



private static Date convertToDate(String p_temp, String pattern)
{
		
		Date dt=null;
		try
		{
			ParsePosition porpos=new ParsePosition(0);
			SimpleDateFormat sdf= new SimpleDateFormat(pattern);
			dt=sdf.parse(p_temp,porpos);
		}
		catch(Exception exception)
		{
			exception.printStackTrace();
		}
		return dt;
}</class_source>
