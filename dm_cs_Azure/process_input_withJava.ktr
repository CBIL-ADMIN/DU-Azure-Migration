        <class_source>import java.util.Map;
import java.util.List;
import java.util.ArrayList;
import com.ericsson.dm.inititialization.LoadSubscriberMapping;
import com.ericsson.dm.transformation.ExecuteTransformation;

int counter=0;
int maximum_write_counter = 100000;

String sdpid, configPath, dataFolderPath, workingPath, output, pathtoApplicationContext, pathOfLogFolder = null;

LoadSubscriberMapping loadMappingObject;
ExecuteTransformation transformer;

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException {
  if (first) {
    first = false;
    
  }

  Object[] r = getRow();

  if (r == null) {
    setOutputDone();
    return false;
  }
  // It is always safest to call createOutputRow() to ensure that your output row's Object[] is large
  // enough to handle any new fields you are creating in this step.
  r = createOutputRow(r, data.outputRowMeta.size());


  String subscriber_xml = get(Fields.In, "subscriber_xml").getString(r);
  try {			
	
    transformer.businessLogicImplementation(subscriber_xml);
    //transformer.populateInput(subscriber_xml);
    //logBasic(subscriber_xml);
	counter++;
    if(counter>=maximum_write_counter)
	{
		logBasic("Going to write output and clear buffer: " + counter);
		//transformer.GenerateOutput();
		transformer.generateCsv();
		//transformer.generateLogs();
		transformer.clearBuffer();
		counter=0;
	}
	
  
  }

  catch(Exception e){

	logError("Error :"+subscriber_xml,e);
  }

  // Send the row on to the next step.
  putRow(data.outputRowMeta, r);


	
  return true;
}



public boolean init(StepMetaInterface stepMetaInterface, StepDataInterface stepDataInterface) {
  
	dataFolderPath = getVariable("KETTLE_HOME") + "/data";
	configPath = getVariable("KETTLE_HOME") + "/config";
	workingPath = getVariable("KETTLE_HOME") + "/working/";
	output = getVariable("KETTLE_HOME") + "/output/";
	pathtoApplicationContext = getVariable("KETTLE_HOME") + "/";
	pathOfLogFolder = getVariable("KETTLE_HOME") + "/logs/";
	sdpid = "dummy";
	maximum_write_counter=Integer.parseInt(getVariable("OUTPUT_WRITE_COUNT"));
	logBasic("Maximum Write Counter: " + maximum_write_counter);

	loadMappingObject = new LoadSubscriberMapping(sdpid, configPath, dataFolderPath, workingPath);
    transformer = new ExecuteTransformation(workingPath, pathtoApplicationContext, pathOfLogFolder);

	return parent.initImpl(stepMetaInterface, stepDataInterface);
}


public void dispose(StepMetaInterface smi, StepDataInterface sdi) {
  
	transformer.generateCsv();
	transformer.clearBuffer();
	parent.disposeImpl(smi, sdi);
}</class_source>
