        <class_source>import com.ericsson.LoadInputDump.LoadInputDump;
import java.util.*;
import java.io.File;

private String dumpFilePath;
private String schemaFilePath;
private String mapdbPath;
private String sdpId;
private String migToolPath;

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException {
  if (first) {
    first = false;
   
  }

  Object[] r = getRow();

  if (r == null) {
    setOutputDone();
    return false;
  }

  // It is always safest to call createOutputRow() to ensure that your output row's Object[] is large
  // enough to handle any new fields you are creating in this step.
  r = createOutputRow(r, data.outputRowMeta.size());

  String MSISDN = get(Fields.In, "MSISDN").getString(r);
  //logBasic(MSISDN);

  
  String SubscriberInput = "&lt;subscriber_info>" + get(Fields.In, "SubscriberInput").getString(r) + "&lt;/subscriber_info>";
  
  LoadInputDump lid = new LoadInputDump();

  String subscriber_xml = "&lt;subscriber_xml>" + SubscriberInput + lid.GetXML(MSISDN) + "&lt;/subscriber_xml>";

  get(Fields.Out, "subscriber_xml").setValue(r, subscriber_xml);

  // Send the row on to the next step.
  putRow(data.outputRowMeta, r);

  return true;
}


public boolean init(StepMetaInterface stepMetaInterface, StepDataInterface stepDataInterface) {
 
   //filesToBeInserted = "sdp01_subscriber_balances_dump.csv.gz,sdp01_subscriber_cugcli_dump.csv.gz,sdp01_subscriber_usms_dump.csv.gz";
   sdpId = "sdp01";
  

   dumpFilePath = getVariable("KETTLE_HOME").toString() + "/" + getVariable("INPUT_DIR").toString();
   schemaFilePath = getVariable("KETTLE_HOME").toString() + "/config/schema";
   mapdbPath = getVariable("KETTLE_HOME") + "/" + getVariable("WORKING_DIR").toString() ;
   migToolPath = getVariable("KETTLE_HOME") + "/" ;

   logBasic(dumpFilePath + "");
   logBasic(schemaFilePath + "");
   logBasic(mapdbPath + "");
   

   //File file = new File(dumpFilePath);
  
   logBasic("Starting load to MapDB");

   LoadInputDump.execute(dumpFilePath, schemaFilePath, mapdbPath, migToolPath);  
   LoadInputDump.InitializeBtree();

   logBasic("Data loaded to MapDB");
   return parent.initImpl(stepMetaInterface, stepDataInterface); 
}


public void dispose(StepMetaInterface smi, StepDataInterface sdi) {

  LoadInputDump.clearDB();
  parent.disposeImpl(smi, sdi);
}
