        <class_source>
import java.io.File;
import java.io.FileFilter;
import java.io.InputStream;
import java.io.FileInputStream;
import java.util.Vector;
import com.jcraft.jsch.Channel;
import com.jcraft.jsch.ChannelSftp;
import com.jcraft.jsch.ChannelSftp.LsEntry;
import com.jcraft.jsch.JSch;
import com.jcraft.jsch.JSchException;
import com.jcraft.jsch.Session;
import com.jcraft.jsch.ChannelExec;
import com.jcraft.jsch.ChannelShell;
import org.apache.commons.io.filefilter.WildcardFileFilter;

private String KETTLE_HOME=null;
private String INPUT_DIR=null;
private String OUTPUT_DIR=null;
private String WORKING_DIR=null;

private int threadSleepTime=50;

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException {

	Object[] r=getRow();

		if (r == null)
		{
			setOutputDone();
			return false;
		}

	StringBuffer scriptOutput=new StringBuffer();

	String IP = get(Fields.In, "IP").getString(r);
	int PORT = Integer.parseInt(get(Fields.In, "PORT").getString(r));
	String USERNAME = get(Fields.In, "USERNAME").getString(r);
	String PASSWORD = get(Fields.In, "PASSWORD").getString(r);
	String NODE_ID = get(Fields.In, "NODE_ID").getString(r);


	String MIG_TOOL_PATH = get(Fields.In, "MIG_TOOL_PATH").getString(r)+INPUT_DIR+File.separator;
	String LOCAL_FOLDER_PATH = KETTLE_HOME+File.separator+WORKING_DIR;
	logBasic("MIG_TOOL_PATH file:  "+get(Fields.In, "MIG_TOOL_PATH").getString(r)+"\n");	

	String SDP_SUBSCRIBER_BALANCES1 = NODE_ID+"_subscriber_balances_dump_01.csv.gz";
	String SDP_SUBSCRIBER_BALANCES_FILENAME1 = LOCAL_FOLDER_PATH+File.separator+SDP_SUBSCRIBER_BALANCES1;
	logBasic("SDP balance file:  "+SDP_SUBSCRIBER_BALANCES_FILENAME1+"\n");
	
	String SDP_SUBSCRIBER_BALANCES2 = NODE_ID+"_subscriber_balances_dump_02.csv.gz";
	String SDP_SUBSCRIBER_BALANCES_FILENAME2 = LOCAL_FOLDER_PATH+File.separator+SDP_SUBSCRIBER_BALANCES2;
	logBasic("SDP balance file:  "+SDP_SUBSCRIBER_BALANCES_FILENAME2+"\n");
	
	String SDP_SUBSCRIBER_BALANCES3 = NODE_ID+"_subscriber_balances_dump_03.csv.gz";
	String SDP_SUBSCRIBER_BALANCES_FILENAME3 = LOCAL_FOLDER_PATH+File.separator+SDP_SUBSCRIBER_BALANCES3;
	logBasic("SDP balance file:  "+SDP_SUBSCRIBER_BALANCES_FILENAME3+"\n");
	
	String SDP_SUBSCRIBER_BALANCES4 = NODE_ID+"_subscriber_balances_dump_04.csv.gz";
	String SDP_SUBSCRIBER_BALANCES_FILENAME4 = LOCAL_FOLDER_PATH+File.separator+SDP_SUBSCRIBER_BALANCES4;
	logBasic("SDP balance file:  "+SDP_SUBSCRIBER_BALANCES_FILENAME4+"\n");

	String SDP_SUBSCRIBER_CUGCLI = NODE_ID+"_subscriber_cugcli_dump.csv.gz";
	String SDP_SUBSCRIBER_CUGCLI_FILENAME = LOCAL_FOLDER_PATH+File.separator+SDP_SUBSCRIBER_CUGCLI;
	logBasic("SDP CUGCLI file:  "+SDP_SUBSCRIBER_CUGCLI_FILENAME+"\n");

	String SDP_SUBSCRIBER_BE = NODE_ID+"_subscriber_be_dump.csv.gz";
	String SDP_SUBSCRIBER_BE_FILENAME = LOCAL_FOLDER_PATH+File.separator+SDP_SUBSCRIBER_BE;
	logBasic("SDP BE file:  "+SDP_SUBSCRIBER_BE_FILENAME+"\n");
	
	String SDP_SUBSCRIBER_PROFILE = NODE_ID+"_subscriber_profile_dump.csv.gz";
	String SDP_SUBSCRIBER_PROFILE_FILENAME = LOCAL_FOLDER_PATH+File.separator+SDP_SUBSCRIBER_PROFILE;
	logBasic("SDP Profile file:  "+SDP_SUBSCRIBER_PROFILE_FILENAME+"\n");
	
	String SDP_SUBSCRIBER_USMS = NODE_ID+"_subscriber_usms_dump.csv.gz";
	String SDP_SUBSCRIBER_USMS_FILENAME = LOCAL_FOLDER_PATH+File.separator+SDP_SUBSCRIBER_USMS;
	logBasic("SDP USMS file:  "+SDP_SUBSCRIBER_USMS_FILENAME+"\n");
	
	
	JSch jsch=null;
	Session session=null;
	java.util.Properties config=null;
	ChannelSftp sftpChannel=null;
	Channel channel=null;
	InputStream in=null,error=null;
	File file=null;
	File ecms=null;


	try
	{
		jsch=new JSch();
		session=jsch.getSession(USERNAME, IP, PORT);
			session.setPassword(PASSWORD);

		config = new java.util.Properties(); 
		config.put("StrictHostKeyChecking", "no");
		session.setConfig("PreferredAuthentications", 
	                  "password");

		session.setConfig(config);
		session.connect();

		channel=session.openChannel("sftp");
		session.setServerAliveInterval(92000);
		channel.connect();
		
		sftpChannel = (ChannelSftp) channel;
		
		scriptOutput.append("Uploading @ Node="+NODE_ID+", Host IP="+IP+", Port="+PORT+",Username="+USERNAME+", Remote Path="+MIG_TOOL_PATH+",Local Folder="+LOCAL_FOLDER_PATH+"\n");	
		logBasic("Uploading @ Node="+NODE_ID+", Host IP="+IP+", Port="+PORT+",Username="+USERNAME+", Remote Path="+MIG_TOOL_PATH+",Local Folder="+LOCAL_FOLDER_PATH+"\n");	


		file=new File(SDP_SUBSCRIBER_BALANCES_FILENAME1);
		if (file.exists() &amp;&amp; !file.isDirectory())
		{
			scriptOutput.append("	Files Uploaded="+SDP_SUBSCRIBER_BALANCES_FILENAME1+"\n");
			sftpChannel.put(new FileInputStream(file), MIG_TOOL_PATH+SDP_SUBSCRIBER_BALANCES1, ChannelSftp.OVERWRITE);
		}
		
		file=new File(SDP_SUBSCRIBER_BALANCES_FILENAME2);
		if (file.exists() &amp;&amp; !file.isDirectory())
		{
			scriptOutput.append("	Files Uploaded="+SDP_SUBSCRIBER_BALANCES_FILENAME2+"\n");
			sftpChannel.put(new FileInputStream(file), MIG_TOOL_PATH+SDP_SUBSCRIBER_BALANCES2, ChannelSftp.OVERWRITE);
		}
		
		file=new File(SDP_SUBSCRIBER_BALANCES_FILENAME3);
		if (file.exists() &amp;&amp; !file.isDirectory())
		{
			scriptOutput.append("	Files Uploaded="+SDP_SUBSCRIBER_BALANCES_FILENAME3+"\n");
			sftpChannel.put(new FileInputStream(file), MIG_TOOL_PATH+SDP_SUBSCRIBER_BALANCES3, ChannelSftp.OVERWRITE);
		}
		
		file=new File(SDP_SUBSCRIBER_BALANCES_FILENAME4);
		if (file.exists() &amp;&amp; !file.isDirectory())
		{
			scriptOutput.append("	Files Uploaded="+SDP_SUBSCRIBER_BALANCES_FILENAME4+"\n");
			sftpChannel.put(new FileInputStream(file), MIG_TOOL_PATH+SDP_SUBSCRIBER_BALANCES4, ChannelSftp.OVERWRITE);
		}
		
		file=new File(SDP_SUBSCRIBER_BE_FILENAME);
		if (file.exists() &amp;&amp; !file.isDirectory())
		{
			scriptOutput.append("	Files Uploaded="+SDP_SUBSCRIBER_BE_FILENAME+"\n");
			sftpChannel.put(new FileInputStream(file), MIG_TOOL_PATH+SDP_SUBSCRIBER_BE, ChannelSftp.OVERWRITE);
		}
		
		file=new File(SDP_SUBSCRIBER_CUGCLI_FILENAME);
		if (file.exists() &amp;&amp; !file.isDirectory())
		{
			scriptOutput.append("	Files Uploaded="+SDP_SUBSCRIBER_CUGCLI_FILENAME+"\n");
			sftpChannel.put(new FileInputStream(file), MIG_TOOL_PATH+SDP_SUBSCRIBER_CUGCLI, ChannelSftp.OVERWRITE);
		}
		
		file=new File(SDP_SUBSCRIBER_PROFILE_FILENAME);
		if (file.exists() &amp;&amp; !file.isDirectory())
		{
			scriptOutput.append("	Files Uploaded="+SDP_SUBSCRIBER_PROFILE_FILENAME+"\n");
			sftpChannel.put(new FileInputStream(file), MIG_TOOL_PATH+SDP_SUBSCRIBER_PROFILE, ChannelSftp.OVERWRITE);
		}
		
		file=new File(SDP_SUBSCRIBER_USMS_FILENAME);
		if (file.exists() &amp;&amp; !file.isDirectory())
		{
			scriptOutput.append("	Files Uploaded="+SDP_SUBSCRIBER_USMS_FILENAME+"\n");
			sftpChannel.put(new FileInputStream(file), MIG_TOOL_PATH+SDP_SUBSCRIBER_USMS, ChannelSftp.OVERWRITE);
		}
		

		sftpChannel.exit();
		session.disconnect();
		
		//logBasic(scriptOutput.toString());

		r = createOutputRow(r, data.outputRowMeta.size());
				get(Fields.Out, "STATUS").setValue(r, scriptOutput.toString());
		putRow(data.outputRowMeta, r);
	}
	catch(Exception exception)
	{
		logError(exception.getMessage(), exception);
	}
	finally
	{
		jsch=null;
		config=null;
		session=null;
		sftpChannel=null;
		in=null;error=null;
	}
	return true;
}


public boolean init(StepMetaInterface stepMetaInterface, StepDataInterface stepDataInterface)
{
	super.init(stepMetaInterface, stepDataInterface);
	KETTLE_HOME=getVariable("KETTLE_HOME","");
	INPUT_DIR=getVariable("INPUT_DIR","input");
	OUTPUT_DIR=getVariable("OUTPUT_DIR","output");
	WORKING_DIR=getVariable("WORKING_DIR", "working");
	
	return true;
}

public void dispose(StepMetaInterface smi, StepDataInterface sdi)
{
		parent.disposeImpl(smi, sdi);
}
