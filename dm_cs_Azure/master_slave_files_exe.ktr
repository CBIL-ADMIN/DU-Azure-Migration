    <definitions>
      <definition>
        <class_type>TRANSFORM_CLASS</class_type>
        <class_name>Processor</class_name>
        <class_source>import com.ericsson.datamigration.dmo.sshconnection.ConnectionMapper;
import com.ericsson.datamigration.dmo.sshconnection.ConnectionWrapper;
import com.ericsson.datamigration.dmo.util.DMOUtilities;
import java.util.HashMap;
import java.io.File;

private ConnectionMapper taskConnectionMapper;

private String KETTLE_HOME=null;
private String LOGS_DIR=null;
private String SLAVE_WORKING_MODE=null;
private String STAND_ALONE_FLAG=null;
private String MAPPING_INPUT_FILE=null;

public boolean init(StepMetaInterface stepMetaInterface, StepDataInterface stepDataInterface)
{
	taskConnectionMapper = ConnectionMapper.getInstance();
	super.init(stepMetaInterface, stepDataInterface);	
	KETTLE_HOME=getVariable("KETTLE_HOME","");
	LOGS_DIR=getVariable("LOGS_DIR","");
	SLAVE_WORKING_MODE=getVariable("SLAVE_WORKING_MODE","");
	STAND_ALONE_FLAG=getVariable("STAND_ALONE_FLAG","");
	MAPPING_INPUT_FILE=getVariable("SUBSCRIBER_MAPPING_INPUT_FILE","");
    return true;
}

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
	Object[] r = getRow();
	if (r == null) {
		setOutputDone();
		return false;
	}

	
	String IP = get(Fields.In, "IP").getString(r);
	String PORT = get(Fields.In, "PORT").getString(r);
	String connectionKey = "c_" + IP + ":" + PORT;
	String workingDirectory = get(Fields.In, "MIG_TOOL_PATH").getString(r);
	String user = get(Fields.In, "USERNAME").getString(r);
	String password = get(Fields.In, "PASSWORD").getString(r);
	String NODE_ID = get(Fields.In, "NODE_ID").getString(r);
    
	//ssh parameters
	String NICE_VALUE = "0";
	String IONICE_VALUE = "2";
	String SSH_CONNECTION_TIMEOUT_MS = "0";
	String SSH_CONNECTION_RETRY_TIMEOUT_MS = "600";
	String SSH_MAX_CONNECTION_TRIES = "1";
	String AFFINITY_VALUE = "0";

	String JAVA_COMMAND = get(Fields.In, "JAVA_COMMAND").getString(r);
	String MIG_TOOL_COMMAND = get(Fields.In, "MIG_TOOL_COMMAND").getString(r);
	String MIG_TOOL_PATH = get(Fields.In, "MIG_TOOL_PATH").getString(r);
	String SDP_INPUT_FILENAME = NODE_ID+"_subscriber_dump.gz";
	String MIG_TOOL_MEM = get(Fields.In, "MIG_TOOL_MEM").getString(r);
	String FILENAME=KETTLE_HOME+File.separator+LOGS_DIR+File.separator+NODE_ID+"_"+SLAVE_WORKING_MODE+"_execution.log";
		
	String command = "bash -c \" cd "+ MIG_TOOL_PATH+" ; "+JAVA_COMMAND+" ; "+MIG_TOOL_PATH+MIG_TOOL_COMMAND+" WORKING_MODE="+SLAVE_WORKING_MODE+" STAND_ALONE_FLAG=Y NODE_ID="+NODE_ID+" SUBSCRIBER_MAPPING_INPUT_FILE="+MAPPING_INPUT_FILE+" -Xmx"+MIG_TOOL_MEM+"\"";
	String execCmd = command;

	try {
		
		// Prepare the SSH Connection 
		//logBasic(connectionKey+","+user+","+password+","+SSH_CONNECTION_TIMEOUT_MS+","+SSH_CONNECTION_RETRY_TIMEOUT_MS+","+SSH_MAX_CONNECTION_TRIES);
		ConnectionWrapper cw = (ConnectionWrapper)taskConnectionMapper.getConnection(connectionKey, user, password);
		cw.setConnectionTimeout(Integer.parseInt(SSH_CONNECTION_TIMEOUT_MS));
		cw.setConnectionRetryTimeout(Integer.parseInt(SSH_CONNECTION_RETRY_TIMEOUT_MS));
		cw.setMaxConnectionTries(Integer.parseInt(SSH_MAX_CONNECTION_TRIES));
	
		// Execute the command 
		logBasic(" Command ssh : "+command);
		String resCmd = cw.doCmd(command, workingDirectory, false);
		//String resCmd = cw.doCmd(command, workingDirectory, false, Integer.parseInt(NICE_VALUE), Integer.parseInt(IONICE_VALUE), Integer.parseInt(AFFINITY_VALUE));

		// Get the exit status and the executed command from resonse 
		String[] resCmdSplitted = DMOUtilities.splitResCmd(resCmd);
		String exitStatus = resCmdSplitted[0].substring(resCmdSplitted[0].indexOf(":")+1).trim();
		execCmd = resCmdSplitted[2];
		if (!(exitStatus.equals("0"))){
			throw new Exception(resCmdSplitted[0] + DMOUtilities.CR_LF + resCmdSplitted[1]);
		}

		// Add them to the output if needed
		get(Fields.Out, "OUT_CMD").setValue(r, resCmdSplitted[0] + DMOUtilities.CR_LF + resCmdSplitted[1]);
		get(Fields.Out, "EXEC_CMD").setValue(r, execCmd);	
		get(Fields.Out, "FILENAME").setValue(r, FILENAME);	
		putRow(data.outputRowMeta, r);

	}	
	catch(Exception e)
	{
		logError(Const.getStackTracker(e));
		get(Fields.Out, "EXEC_CMD").setValue(r, execCmd);
		String error_desc = "ERROR::" + e.getMessage();

		putError(data.outputRowMeta, r, 1, error_desc, "error_desc", null);
	}


	return true;
}

public void dispose(StepMetaInterface smi, StepDataInterface sdi)
{
    parent.disposeImpl(smi, sdi);
}</class_source>
