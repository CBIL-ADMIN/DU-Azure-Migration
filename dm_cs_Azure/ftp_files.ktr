private int threadSleepTime=50;

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException {

	Object[] r=getRow();

    if (r == null)
    {
      setOutputDone();
      return false;
    }

	StringBuffer scriptOutput=new StringBuffer();

	String IP = get(Fields.In, "IP").getString(r);
	int PORT = Integer.parseInt(get(Fields.In, "PORT").getString(r));
	String USERNAME = get(Fields.In, "USERNAME").getString(r);
	String PASSWORD = get(Fields.In, "PASSWORD").getString(r);
	String DUMP_REMOTE_PATH = get(Fields.In, "DUMP_REMOTE_PATH").getString(r);
	String LOCAL_FOLDER_PATH = get(Fields.In, "LOCAL_FOLDER_PATH").getString(r);
	String DAT_FILE_PATH = get(Fields.In, "DAT_FILE_PATH").getString(r);
	String AF_FILE_PATH = get(Fields.In, "AF_FILE_PATH").getString(r);
	String NODE_ID = get(Fields.In, "NODE_ID").getString(r);
	
	JSch jsch=null;
	Session session=null;
	java.util.Properties config=null;
	ChannelSftp sftpChannel=null;
	Channel channel=null;
	InputStream in=null,error=null;
//	File file=null;


	try
	{
		jsch=new JSch();
		session=jsch.getSession(USERNAME, IP, PORT);
			session.setPassword(PASSWORD);

		config = new java.util.Properties(); 
		config.put("StrictHostKeyChecking", "no");
		session.setConfig("PreferredAuthentications", 
	                  "password");

		session.setConfig(config);
		session.connect();

		channel=session.openChannel("sftp");
		session.setServerAliveInterval(92000);
		channel.connect();
		
		sftpChannel = (ChannelSftp) channel;
		
		scriptOutput.append("Uploading @ Node="+NODE_ID+", Host IP="+IP+", Port="+PORT+",Username="+USERNAME+", Remote Path="+DUMP_REMOTE_PATH+",Local Folder="+LOCAL_FOLDER_PATH+"\n");	

		File fileSDP=new File(LOCAL_FOLDER_PATH+File.separator+"SDP");
		if (fileSDP.isDirectory())
		{
			FileFilter file1Filter=new WildcardFileFilter("*.csv");
			File[] files=fileSDP.listFiles(file1Filter);
			logBasic("Number of CSV files ="+files.length);
			//executeLinuxCommand(session,scriptOutput,"mkdir "+DUMP_REMOTE_PATH);	
			for (int i=0;i<files.length;i++)
			{
				sftpChannel.put(new FileInputStream(files[i]), DUMP_REMOTE_PATH+files[i].getName(),ChannelSftp.OVERWRITE);
				scriptOutput.append("	File Uploaded="+files[i].getName()+"\n");
			}
		}

		File fileMINSAT=new File(LOCAL_FOLDER_PATH+File.separator+"MINSAT");
		if (fileMINSAT.isDirectory())
		{	
			FileFilter file2Filter=new WildcardFileFilter("*.DAT");
			File[] files=fileMINSAT.listFiles(file2Filter);
			logBasic("Number of DAT files ="+files.length);
//			executeLinuxCommand(session,scriptOutput,"rm -rf "+DAT_FILE_PATH);
//			executeLinuxCommand(session,scriptOutput,"mkdir "+DAT_FILE_PATH);	
			for (int i=0;i<files.length;i++)
			{
				logBasic("	Uploading @ "+DAT_FILE_PATH+files[i].getName());
				sftpChannel.put(new FileInputStream(files[i]), DAT_FILE_PATH+files[i].getName(),ChannelSftp.OVERWRITE);
				scriptOutput.append("	File Uploaded="+files[i].getName()+"\n");
			}
		}

/*
		File fileAF=new File(LOCAL_FOLDER_PATH+File.separator+"AF");
		if (fileAF.isDirectory())
		{	
			FileFilter file3Filter=new WildcardFileFilter("*.msisdn");
			File[] files=fileAF.listFiles(file3Filter);
			logBasic("Number of AF files ="+files.length);
//			executeLinuxCommand(session,scriptOutput,"rm -rf "+AF_FILE_PATH);
//			executeLinuxCommand(session,scriptOutput,"mkdir "+AF_FILE_PATH);	
			for (int i=0;i<files.length;i++)
			{
				sftpChannel.put(new FileInputStream(files[i]), AF_FILE_PATH+files[i].getName(),ChannelSftp.OVERWRITE);
				scriptOutput.append("	File Uploaded="+files[i].getName()+"\n");
			}
			
		}
*/

		
		sftpChannel.exit();
		session.disconnect();
		
		//logBasic(scriptOutput.toString());

		r = createOutputRow(r, data.outputRowMeta.size());
        get(Fields.Out, "STATUS").setValue(r, scriptOutput.toString());
		putRow(data.outputRowMeta, r);
	}
	catch(Exception exception)
	{
		logError(exception.getMessage(), exception);
	}
	finally
	{
		jsch=null;
		config=null;
		session=null;
		sftpChannel=null;
		in=null;error=null;
	}
	return true;
}


