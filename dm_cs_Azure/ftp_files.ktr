{
    parent.disposeImpl(smi, sdi);
}


private void executeLinuxCommand(Session p_Session, StringBuffer p_scriptOutput, String p_Command)
{
	Channel channel=null;
	InputStream in=null,error=null;
	try
	{
		logBasic("Linux Command = "+p_Command);

		channel=p_Session.openChannel("exec");
		((ChannelExec)channel).setCommand(p_Command);
		    
		in=channel.getInputStream();
		error=((ChannelExec)channel).getErrStream();
		channel.connect();

		byte[] tmp=new byte[1024];
	    p_scriptOutput.append("Error Ouput:\n");

	    boolean flag=true;
		while(flag)
	    {
	    	while(error.available()>0)
	    	{
	    		int i=error.read(tmp, 0, 1024);
	    		if(i<0)	break;
	    		p_scriptOutput.append(new String(tmp, 0, i));
	    	}
	    	if(channel.isClosed()) flag=false;
	    	try
			{
				Thread.sleep(threadSleepTime);
			}
			catch(Exception exception)
			{
				logError(exception.getMessage(), exception);
			}
	    }
	     
		p_scriptOutput.append("Log Ouput:\n");

		flag=true;
	    
		while(flag)
	 	{
	     	while(in.available()>0)
	    	{
	    		int i=in.read(tmp, 0, 1024);
	    		if(i<0)	break;
	    		p_scriptOutput.append(new String(tmp, 0, i));
	    	}
	    	if(channel.isClosed()) flag=false;
	    	try
			{
				Thread.sleep(threadSleepTime);
			}
			catch(Exception exception)
			{
				logError(exception.getMessage(), exception);
			}
	    }
		channel.disconnect();
	}
	catch(Exception exception)
	{
		logError(exception.getMessage(), exception);
	}
	finally
	{
		channel=null;
	}
}
